<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
  <Document>
    <name>Mouse-over styles</name>
    <open>1</open>

    <StyleMap id="my_style">
      <Pair>
        <key>normal</key>
        <styleUrl>#style_normal</styleUrl>
      </Pair>
      <Pair>
        <key>highlight</key>
        <styleUrl>#style_normal</styleUrl>
      </Pair>
    </StyleMap>

    <Style id="style_normal">
      <IconStyle>
        <color>ffffffff</color>
        <scale>1</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/shapes/homegardenbusiness.png</href>
        </Icon>
      </IconStyle>
      <LabelStyle>
        <scale>1</scale>
      </LabelStyle>
    </Style>

    <Placemark>
      <name>Make me cool!</name>
      <description>Wassup?</description>
      <Point>
        <coordinates>0,0</coordinates>
      </Point>    
    </Placemark>

  </Document>
</kml>




        