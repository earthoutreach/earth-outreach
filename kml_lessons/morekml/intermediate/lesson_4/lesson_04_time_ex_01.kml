<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
  <Document>
    <name>Make a Timestamp</name>
    <open>1</open>

    <!--
      Here are valid date and time formats:
        https://developers.google.com/kml/documentation/kmlreference#timestamp
      
      - Year: YYYY
        <when>1997</when>
        
      - Year & month: YYYY-MM
        <when>1997-07</when>
        
      - Date: YYYY-MM-DD
        <when>1997-07-16</when>
        
      - Date & time: YYYY-MM-DDThh:mm:ssZ
        <when>1997-07-16T07:30:15Z</when>
      
      - Date, time & time zone: YYYY-MM-DDThh:mm:sszzzzzz
        <when>1997-07-16T10:30:15+03:00</when>    
        
      Here are templates for your TimeStamps/TimeSpans
      
      <TimeStamp>
        <when></when>
      </TimeStamp>
      
      
      <TimeSpan>
        <begin></begin>
        <end></end>
      </TimeSpan>
    
    -->

    <Placemark>
      <name>Pick a date</name>
      <!-- Time data goes here -->
      <Point>
        <coordinates>-122.47,37.755,0</coordinates>
      </Point> 
    </Placemark>

    <Placemark>
      <name>Pick a date range</name>
      <!-- Time data goes here -->
      <Polygon>
        <tessellate>1</tessellate>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
						-122.5028599615374,37.70771366836495,0 -122.3922914095602,37.70795880345399,0 -122.3639186368558,37.71602644232831,0 -122.3556641717737,37.72841692631626,0 -122.3753855358625,37.75167821460732,0 -122.3812746929178,37.75539288561824,0 -122.3847474559409,37.76948119967275,0 -122.387898956887,37.77872114899706,0 -122.3868825844092,37.79019408417859,0 -122.4046317068854,37.80882537338713,0 -122.412454381035,37.81011226642525,0 -122.462885186056,37.805354101423,0 -122.4763884629261,37.81259301806841,0 -122.4864423827431,37.79045132269719,0 -122.4917002347541,37.78798540940206,0 -122.5048287491523,37.78769336466037,0 -122.5148484643782,37.78147325161155,0 -122.511136505453,37.74908592209953,0 -122.5028599615374,37.70771366836495,0 
